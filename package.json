    "vite": "^3.0.9",
    "vite-plugin-checker": "^0.4.2",
    "vite-plugin-stylelint": "^1.2.0",
    "vite-plugin-vue-type-imports": "^0.1.3",
    "vitest": "^0.14.1",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.40.4"
