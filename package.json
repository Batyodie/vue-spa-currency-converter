{
  "name": "vue-spa-currency-converter",
  "version": "0.0.1",
  "description": "vue spa app for currency cpnverter",
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "lint:style": "stylelint \"**/*.{css,scss,sass}\"",
    "lint:style:fix": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "format": "prettier .  --write",
    "tests": "vitest",
    "coverage": "vitest run --coverage",
    "postinstall": "npx simple-git-hooks"
  },
  "keywords": [],
  "author": "Simens",
  "license": "ISC",
  "dependencies": {
    "@vue/compiler-sfc": "^3.2.38",
