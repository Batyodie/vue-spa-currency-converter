    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-vue": "^8.5.0",
    "jsdom": "^19.0.0",
    "nano-staged": "^0.8.0",
    "pnpm": "^6.32.1",
    "postcss": "^8.4.7",
    "postcss-html": "^1.3.0",
